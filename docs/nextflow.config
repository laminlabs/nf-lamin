plugins {
  id 'nf-lamin'
}

lamin {
  env = "prod"
  instance = "laminlabs/lamindata"
  api_key = lamin.env == "prod" ? secrets.LAMIN_API_KEY : secrets.LAMIN_STAGING_API_KEY
}

report {
  enabled = true
  overwrite = true
}

trace {
  overwrite = true
}

timeline {
  overwrite = true
}

dag {
  overwrite = true
}
